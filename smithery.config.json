{
    "build": {
        "dockerBuildPath": "."
    },
    "startCommand": {
        "command": [
            "uvicorn",
            "mcp_app.main:app",
            "--host",
            "0.0.0.0",
            "--port",
            "8000"
        ]
    },
    "transport": {
        "type": "http",
        "port": 8000,
        "http": {
            "toolsList": "/tools/list",
            "toolsCall": "/tools/call"
        }
    }
}
